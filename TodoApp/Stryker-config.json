{
  "stryker-config": {
    "project": "src/Todo.Application/Todo.Application.csproj",
    "test-projects": [
      "tests/Todo.UnitTests/Todo.UnitTests.csproj"
    ],
    "reporters": [ "html", "progress" ],
    "thresholds": { "high": 80, "low": 60, "break": 0 },
    "mutate": [
      "**/*.cs"
    ],
    "ignore-methods": [
      "ToString",
      "GetHashCode",
      "Equals"
    ]
  }
}